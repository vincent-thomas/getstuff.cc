{
  "name": "getstuff.cc",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-p build:*",
    "build:app": "next build --no-lint",
    "build:mail-reciever": "node ./packages/mail-reciever/esbuild.js",
    "db:generate": "dotenv -c local -- drizzle-kit generate:pg",
    "db:migrate": "dotenv -c local -- tsx ./schema-migrator.ts",
    "dev": "next dev",
    "check:ci": "biome ci . && tsc --noEmit",
    "test": "dotenv -c local -- cross-env SKIP_ENV_VALIDATION='1' vitest run",
    "start": "next start",
    "cdk:synth": "dotenv -- cdk synth",
    "cdk:destroy": "dotenv -- cdk destroy --all",
    "cdk:deploy": "dotenv -- cdk deploy",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cdk:synth:pipeline": "dotenv -- cdk synth stuff-pipeline",
    "cdk:mutate-pipeline": "dotenv -c local -- cdk deploy stuff-pipeline",
    "cdk:deploy:mail-reciever": "dotenv -c local -- cdk deploy dev-stuff-email-reciever"
  },
